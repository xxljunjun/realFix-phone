{
	"easycom": {
		//引入的组件无需在页面内import，也不需要在components内声明，即可在任意页面使用
		"autoscan": true,
		"custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},
	"globalStyle": {
		"backgroundColor": "#F8F8F8",
		"navigationStyle": "custom", //去除导航栏
		"app-plus": {
			// 将回弹属性关掉
			"bounce": "none"
		}
	},
	"style" : {
			"navigationBarTitleText" : "xxx",
			"app-plus" : {
				"bounce" : "none" // 取消APP端iOS回弹,避免与下拉刷新冲突 (可统一配在 'globalStyle')
			},
			"mp-alipay":{"allowsBounceVertical":"NO"} // 取消支付宝和钉钉小程序的iOS回弹,避免与下拉刷新冲突 (可统一配在 'globalStyle')
		},
