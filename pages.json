{
	"easycom": {
		//引入的组件无需在页面内import，也不需要在components内声明，即可在任意页面使用
		"autoscan": true,
		"custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},
	"pages": [
		{
			"path": "pages/realFix/Featured",
			"style": {
				"navigationStyle": "custom",
				"animationType": "none"
			}
		},
		{
			"path": "pages/realFix/NewPost",
			"style": {
				"navigationStyle": "custom",
				"animationType": "none"
			}
		},
		{
			"path": "pages/realFix/Myself",
			"style": {
				"navigationStyle": "custom",
				"animationType": "none"
			}
		},
		{
			"path": "pages/realFix/Favorite",
			"style": {
				"navigationStyle": "custom",
				"animationType": "fade-in",
        		"animationDuration": 100
			}
		},
		{
			"path": "pages/realFix/Mypost",
			"style": {
				"navigationStyle": "custom",
				"animationType": "fade-in",
        		"animationDuration": 100
			}
		},
		{
			"path": "pages/realFix/detail",
			"style": {
				"navigationStyle": "custom",
				"animationType": "fade-in",
        		"animationDuration": 100
			}
		},
		{
			"path": "pages/realFix/search",
			"style": {
				"navigationStyle": "custom",
				"animationType": "fade-in",
        		"animationDuration": 100
			}
		}
	]
}